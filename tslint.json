{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-console": [
      false
    ],
    "interface-name": [
      false
    ],
  }
}